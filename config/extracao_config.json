    "Cor": "COR[:\\s]*([A-ZÇÃÕÉ ]+)"
  },
  "validadores": {
    "chassi": "^[A-HJ-NPR-Z0-9]{17}$",
    "placa_mercosul": "^[A-Z]{3}[0-9][A-Z0-9][0-9]{2}$",
    "placa_antiga": "^[A-Z]{3}-[0-9]{4}$"
